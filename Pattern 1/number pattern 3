
#include <stdio.h>
int main()
{
    int a;
    printf("Enter no. of lines you want to print\n");
    scanf("%d",&a);
    for (int i=1; i<=a; ++i){
        for (int j =1; j<=i; ++j){
            if (i==1 && j==1){
                printf("1");
                break;
            }
            if (i==j || j==1){
                printf("1");
            }
            else{
                printf("2");
            }
            
        }printf("\n");
    }return 0;
}
